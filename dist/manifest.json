{
  "manifest_version": 3,
  "name": "Geogebra Shortcuts System 1.0",
  "version": "1.0",
  "description": "A chrome extensions that adds a shortcut system to the Geogebra's geometrical calculator.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.geogebra.org/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}