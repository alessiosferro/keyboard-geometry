{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "lib": [
      "ESNext",
      "dom",
      "dom.iterable"
    ]
  },
  "include": [
    "src"
  ]
}